    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.3",
    "cleave.js": "^1.6.0",
    "clsx": "^1.1.1",
    "convert-units": "^2.3.4",
    "dotenv": "^8.2.0",
    "framer-motion": "^3.10.0",
    "google-map-react": "^2.1.9",
    "history": "^4.10.1",
    "html2canvas": "^1.0.0-rc.7",
    "i18next": "^19.8.7",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "node-sass": "^5.0.0",
    "nuka-carousel": "^4.5.12",
